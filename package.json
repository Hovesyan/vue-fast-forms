  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --inline --hot --config config/dev.webpack.config.js",
    "prod": "cross-env NODE_ENV=production webpack --progress --hide-modules --config config/prod.webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules --config config/build.webpack.config.js",
    "serve": "cross-env VUE_ENV=server nodemon --watch . --ext js,html server.js"
